<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">



    <!--When Button is not enabled -->
    <item android:state_enabled="false" android:color="#FF9800" />
    <!--When Button is in pressed state -->
    <item android:state_pressed="true" android:color="#FB9600" />
    <!--When Button is in selected state -->
    <item android:state_selected="true" android:color="#FF9800" />
    <!--Default Background Color -->
    <item android:color="#F1B70A" />


</selector>